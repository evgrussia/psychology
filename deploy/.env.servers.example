# SSH и пути к серверам — НЕ коммитить в git!
# Скопировать в .env.servers в корне репозитория и заполнить.

# Development
DEV_SSH_HOST=dev.example.com
DEV_SSH_USER=deploy
DEV_SSH_KEY_PATH=~/.ssh/psychology_dev
DEV_PROJECT_PATH=/var/www/psychology
DEV_DB_NAME=psychology_dev

# Production
PROD_SSH_HOST=prod.example.com
PROD_SSH_USER=deploy
PROD_SSH_KEY_PATH=~/.ssh/psychology_prod
PROD_PROJECT_PATH=/var/www/psychology
PROD_DB_NAME=psychology_prod
